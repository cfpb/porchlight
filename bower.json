    "cf-icons": "*",
    "cf-layout": "~0.2.2",
    "d3": "~3.5.3",
    "angular": "~1.3.11",
    "angular-sanitize": "~1.3.11",
    "angular-ui-router": "~0.2.13",
    "angular-resource": "~1.3.11",
    "angular-indicator": "ngActivityIndicator#~0.1.10",
    "angular-cookies": "~1.3.11",
    "highcharts-ng": "~0.0.7",
