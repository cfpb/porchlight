{
  "name": "porchlight",
  "version": "0.1.0",
  "description": "Application to display git repo deploy cycle",
  "homepage": "http://consumerfinance.gov/porchlight",
  "author": {
    "name": "Steven Bellamy",
    "email": "steven.bellamy@cfpb.gov",
    "url": "http://consumerfinance.gov"
  },
